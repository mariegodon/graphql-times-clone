{
  "name": "graphql-times-clone",
  "version": "1.0.0",
  "description": "A clone of the New York Times movie reviews using GraphQL.",
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "build": "webpack --mode production",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "apollo-boost": "^0.1.9",
    "apollo-cache-inmemory": "^1.2.4",
    "apollo-client": "^2.3.4",
    "apollo-fetch": "^0.7.0",
    "apollo-link-http": "^1.5.4",
    "apollo-link-rest": "^0.3.1",
    "apollo-server-express": "^1.3.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "body-parser": "^1.18.2",
    "casual": "^1.5.17",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "css-loader": "^0.28.11",
    "express": "^4.16.2",
    "file-loader": "^1.1.11",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^2.13.0",
    "html-webpack-plugin": "^3.2.0",
    "node-fetch": "^1.7.3",
    "react": "^16.4.1",
    "react-apollo": "^2.1.5",
    "react-dom": "^16.4.1",
    "styled-components": "^3.3.2",
    "webpack-cli": "^3.0.8"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "nodemon": "^1.12.1",
    "webpack": "^4.12.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariegodon/graphql-times-clone.git"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-es2015-arrow-functions"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mariegodon/graphql-times-clone/issues"
  },
  "homepage": "https://github.com/mariegodon/graphql-times-clone#readme"
}
